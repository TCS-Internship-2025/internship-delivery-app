{
  "editor.codeLens": true,
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "typescript.referencesCodeLens.enabled": true,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.preferences.autoImportSpecifierExcludeRegexes": ["^@mui/[^/]+$"],
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "files.insertFinalNewline": true,
  "files.autoSave": "afterDelay",
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "eslint.format.enable": true,
  "errorLens.enabled": true,
  "git.autofetch": true,
  "indenticator.inner.showHover": true,
  "indenticator.inner.showIndentGuide": true,
  "indenticator.hover.peekBack": 1,
  "indenticator.inner.hover.peekForward": 1,
  "indenticator.inner.width": 2,
  "indenticator.hover.highlight": true,
  "indenticator.showHover": true,
  "indenticator.inner.color.dark": "#ff8400",
  "indenticator.style": "inset",
  "cSpell.words": ["mapbox"]
}
